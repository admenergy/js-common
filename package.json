{
  "name": "@admenergy/js-common",
  "version": "1.0.0",
  "exports": {
    "./client": {
      "import": "./dist/js/client/index.js",
      "types": "./dist/ts/client/index.d.ts"
    },
    "./common": {
      "import": "./dist/js/common/index.js",
      "types": "./dist/ts/common/index.d.ts"
    },
    ".": {
      "import": "./dist/js/index.js",
      "types": "./dist/ts/index.d.ts"
    }
  },
  "webpackEntry": {
    "client": "./src/client/index.ts",
    "common": "./src/common/index.ts",
    ".": "./src/index.ts"
  },
  "private": true,
  "scripts": {
    "==[ Development ]==": "",
    "build": "npm run clean && webpack --mode production && npx tsc && git add dist/",
    "lint": "npx eslint . --config .eslintrc.js --ext .js,.jsx,.ts,.tsx --format json | node utility/eslint.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage=true",
    "==[ Install/Clean ]==": "",
    "reinstall": "npm run purge && npm i",
    "reinstall-rebuild": "npm run reinstall && npm run build",
    "update": "npx npm-check-updates -u && npm install",
    "clean": "rm -rf dist/ .next/",
    "purge": "rm -rf dist/ .next/ node_modules/ package-lock.json"
  },
  "dependencies": {
    "@azure/msal-browser": "^3.9.0",
    "@azure/msal-node": "^2.6.2",
    "@azure/msal-react": "^2.0.10",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@microsoft/eslint-formatter-sarif": "^3.0.0",
    "@mui/icons-material": "5.15.8",
    "@mui/material": "^5.15.7",
    "@testing-library/jest-dom": "^6.4.2",
    "@types/babel__preset-env": "~7.9.6",
    "@types/eslint": "~8.56.2",
    "@types/lodash": "^4.14.202",
    "@types/lodash-es": "~4.17.12",
    "@types/react": "~18.2.55",
    "@types/react-dom": "~18.2.19",
    "@types/react-test-renderer": "~18.0.7",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "babel-loader": "^9.1.3",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^2.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react-test-renderer": "^18.2.0",
    "typescript": "^5.3.3",
    "typesync": "^0.12.1",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4"
  }
}
