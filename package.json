{
  "name": "@admenergy/js-common",
  "version": "1.0.0",
  "exports": {
    ".": {
      "import": "./dist/js/index.js",
      "types": "./dist/ts/index.d.ts",
      "default": "./dist/js/index.js"
    },
    "./client": {
      "import": "./dist/js/client/index.js",
      "types": "./dist/ts/client/index.d.ts",
      "default": "./dist/js/client/index.js"
    },
    "./common": {
      "import": "./dist/js/common/index.js",
      "types": "./dist/ts/common/index.d.ts",
      "default": "./dist/js/common/index.js"
    },
    "./server": {
      "import": "./dist/js/server/index.js",
      "types": "./dist/ts/server/index.d.ts",
      "default": "./dist/js/server/index.js"
    },
    "./ErrorTypes": {
      "import": "./dist/js/ErrorTypes/index.js",
      "types": "./dist/ts/ErrorTypes/index.d.ts",
      "default": "./dist/js/ErrorTypes/index.js"
    }
  },
  "webpackEntry": {
    ".": "./app/index.ts",
    "client": "./app/client/index.ts",
    "common": "./app/common/index.ts",
    "server": "./app/server/index.ts",
    "ErrorTypes": "./app/ErrorTypes/index.ts"
  },
  "scripts": {
    "==[ Development ]==": "",
    "build": "yarn clean && yarn webpack --mode production && yarn tsc",
    "build-git-add": "yarn build && git add dist/",
    "build-test": "yarn build && vitest run",
    "====[ Lint ]====": "",
    "lint": "yarn eslint app/ && yarn tsc -p tsconfig.test.json",
    "lint:fix": "yarn eslint app/ --fix",
    "====[ Test ]====": "",
    "test": "NODE_ENV=test vitest run",
    "test:changed": "NODE_ENV=test vitest run --changed --reporter=verbose",
    "test:coverage": "NODE_ENV=test vitest run --coverage",
    "==[ Install/Clean ]==": "",
    "install-sdk": "yarn dlx @yarnpkg/sdks vscode",
    "reinstall": "yarn set version stable && yarn purge && yarn install && npx typesync",
    "clean": "rm -rf dist/",
    "purge": "yarn clean && rm -rf node_modules/ package-lock.json yarn.lock"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "json5": "^2.2.3",
    "lodash": "^4.17.23",
    "lodash-es": "^4.17.23",
    "notistack": "^3.0.2",
    "react": "<19.0.0",
    "react-dom": "<19.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/preset-env": "^7.29.0",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@eslint/js": "^9.39.2",
    "@microsoft/eslint-formatter-sarif": "^3.1.0",
    "@mui/icons-material": "<8.0.0",
    "@mui/material": "<8.0.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.2",
    "@types/babel__core": "~7.20.5",
    "@types/babel__preset-env": "^7.10.0",
    "@types/eslint": "^9.6.1",
    "@types/eslint-plugin-jsx-a11y": "^6.10.1",
    "@types/eslint-plugin-security": "~3.0.1",
    "@types/jsdom": "^27.0.0",
    "@types/lodash": "^4.17.23",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^25.2.3",
    "@types/react": "<19.0.0",
    "@types/react-dom": "<19.0.0",
    "@types/react-test-renderer": "<20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "babel-loader": "^10.0.0",
    "eslint": "^10.0.0",
    "eslint-config-next": "^16.1.6",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-unused-imports": "^4.4.1",
    "globals": "^17.3.0",
    "jsdom": "^28.1.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0",
    "typesync": "^0.14.3",
    "vitest": "^4.0.18",
    "webpack": "^5.105.2",
    "webpack-cli": "^6.0.1"
  },
  "packageManager": "yarn@4.12.0"
}
